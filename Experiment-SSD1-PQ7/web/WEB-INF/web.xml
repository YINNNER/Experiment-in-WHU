<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>HtmlTable</servlet-name>
        <servlet-class>HtmlTable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HtmlPage</servlet-name>
        <servlet-class>HtmlPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HtmlAnchor</servlet-name>
        <servlet-class>HtmlAnchor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Song</servlet-name>
        <servlet-class>Song</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Jukebox</servlet-name>
        <servlet-class>Jukebox</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HtmlPage</servlet-name>
        <url-pattern>/HtmlPage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HtmlTable</servlet-name>
        <url-pattern>/HtmlTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HtmlAnchor</servlet-name>
        <url-pattern>/HtmlAnchor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Song</servlet-name>
        <url-pattern>/Song</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Jukebox</servlet-name>
        <url-pattern>/Jukebox</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>JukeboxForm.html</welcome-file>
    </welcome-file-list>
</web-app>